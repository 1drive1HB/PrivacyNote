{
  "model": {
    "chatCompression": {
      "contextPercentageThreshold": 0.75
    }
  },
  "context": {
    "fileFiltering": {
      "respectGitIgnore": true,
      "respectGeminiIgnore": true
    }
  },
  "mcpServers": {
    "context7": {
      "httpUrl": "https://mcp.context7.com/mcp",
      "headers": {
        "Authorization": "@{CONTEXT7_API_KEY}",
        "Accept": "application/json, text/event-stream"
      }
    },
    "exa": {
      "httpUrl": "https://mcp.exa.ai/mcp",
      "headers": {
        "Authorization": "@{EXA_API_KEY_MCP}"
      }
    },
    "github": {
      "httpUrl": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "timeout": 5000
    },
    "gitmcp": {
      "type": "sse",
      "transport": "sse",
      "format": "mcp",
      "url": "https://gitmcp.io/1drive1HB/PrivacyNote/"
    },
    "supabase": {
      "httpUrl": "https://mcp.supabase.com/mcp"
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    }
  },
  "general": {
    "outputFormat": "markdown",
    "timeoutMs": 30000,
    "retries": 3,
    "useCache": true
  },
  "advanced": {
    "logLevel": "info"
  }
}