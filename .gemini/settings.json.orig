{
  "selectedAuthType": "api-key",
  "model": {
    "name": "gemini-2.5-pro"
  },
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        // This passes your API key from the .env file to the context7 process.
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY_MCP}"
      }
    },
    "github": {
      "httpUrl": "https://api.githubcopilot.com/mcp/",
      "headers": {
        // Added 'Bearer ' prefix, which is required for GitHub/Copilot token authentication.
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "timeout": 5000,
      "trust": false 
    },
    "gitmcp": {
      "type": "sse",
      "url": "https://gitmcp.io/1drive1HB/PrivacyNote"
    }
  },
  "settings": {
    "timeout": 30000,
    "retries": 3,
    "logging": true
  },
  "environment": {
    "USE_CACHE": true
  }
}